
struct LogContext {
  trace: (fmt: stringview, ...) => none;
  info: (fmt: stringview, ...) => none;
  debug: (fmt: stringview, ...) => none;
  warn: (fmt: stringview, ...) => none;
  error: (fmt: stringview, ...) => none;
}

struct MemContext {
  alloc: (size: u64) => RawPtr<none>;
}

struct Context {
  log: LogContext;
  mem: MemContext;
};