
struct CString {
}

struct LogContext {
  trace: (fmt: str, ...) => none;
  info: (fmt: str, ...) => none;
  debug: (fmt: str, ...) => none;
  warn: (fmt: str, ...) => none;
  error: (fmt: str, ...) => none;
}

struct MemContext {
  alloc: (size: u64) => none*;
  globalDefaultArena: none*;
}

struct Context {
  log: LogContext;
  mem: MemContext;
};
