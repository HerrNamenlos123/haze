
export struct Result<TSuccess, TError> {
  successValue: TSuccess;
  errorValue: TError;
  isSuccess: boolean;

  static success(value: TSuccess): Result<TSuccess, TError> {
    return Result<TSuccess, TError> {
        isSuccess: true,
        successValue: value,
    };
  }

  static error(value: TError): Result<TSuccess, TError> {
    return Result<TSuccess, TError> {
        isSuccess: false,
        errorValue: value,
    };
  }
}